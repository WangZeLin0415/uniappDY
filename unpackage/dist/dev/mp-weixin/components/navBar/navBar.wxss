
.box {
	width:100%;
	height:45px;
	position: absolute;
	top:30px;
	left:0px;
	color:#fff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	z-index: 20;
}
.search {
	position: absolute;
	top:50%;
	left:30px;
	font-size: 30px;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
}
.nav {
	padding:0 20px;
}

